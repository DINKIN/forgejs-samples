{
    "audio":
    {
        "enabled": true,

        "volume":
        {
            "default": 0.5,
            "max": 1.0
        }
    },

    "playlists":
    {
        "enabled": true,

        "volume":
        {
            "default": 0.5,
            "max": 1.0
        },

        "default": "playlist-1",

        "lists":
        [
            {
                "uid": "playlist-1",
                "name": "Karting Playlist",
                "default": "playlist-1-track-0",
                "loop": true,
                "autoPlay": true,

                "tracks": ["playlist-1-track-0", "playlist-1-track-1", "playlist-1-track-2"]
            }
        ],

        "tracks":
        [
            {
                "uid": "playlist-1-track-0",
                "author": "",
                "name": "",
                "url": "https://cdn.forgejs.org/samples/vrrroom/audio/Double-Trouble.mp3"
            },

            {
                "uid": "playlist-1-track-1",
                "author": "",
                "name": "",
                "url": "https://cdn.forgejs.org/samples/vrrroom/audio/dexters-lab.mp3"
            },

            {
                "uid": "playlist-1-track-2",
                "author": "",
                "name": "",
                "url": "https://cdn.forgejs.org/samples/vrrroom/audio/Conscious.mp3"
            }
        ]
    },

    "story":
    {
        "uid": "story-0",
        "name": "Karting Race",
        "slug": "kart-srace",
        "description": "This project have multiple synchronized videos scenes.",
        "default": "scene-0",

        "scenes":
        [
            {
                "uid": "scene-0",
                "name": "Samy Karting",
                "slug": "samy-karting",
                "description": "Samy Karting Session",
                "sync":["scene-1", "scene-2", "scene-3"],

                "media":
                {
                    "uid": "media-0",
                    "type": "video",

                    "source":
                    {
                        "format": "equi",
                        "streaming": "dash",
                        "url": "https://cdn.forgejs.org/samples/vrrroom/videos/460017/460017.mpd"
                    },

                    "options":
                    {
                        "autoPlay": true,
                        "loop": true,
                        "volume": 0
                    }
                },

                "view":
                {
                    "type": "GoPro"
                }
            },

            {
                "uid": "scene-1",
                "name": "Martin Karting",
                "slug": "martin-karting",
                "description": "Martin Karting Session",
                "sync":["scene-0", "scene-2", "scene-3"],

                "media":
                {
                    "uid": "media-1",
                    "type": "video",

                    "source":
                    {
                        "format": "equi",
                        "streaming": "dash",
                        "url": "https://cdn.forgejs.org/samples/vrrroom/videos/459989/459989.mpd"
                    },

                    "options":
                    {
                        "autoPlay": true,
                        "loop": true,
                        "volume": 0
                    }
                },

                "view":
                {
                    "type": "GoPro"
                }
            },

            {
                "uid": "scene-2",
                "name": "Samy Karting - Cinema Mode",
                "slug": "samy-karting-cinema",
                "description": "Samy Karting Session Edit",
                "sync":["scene-0", "scene-1", "scene-3"],

                "media":
                {
                    "uid": "media-2",
                    "type": "video",

                    "source":
                    {
                        "format": "equi",
                        "streaming": "dash",
                        "url": "https://cdn.forgejs.org/samples/vrrroom/videos/460036/460036.mpd"
                    },

                    "options":
                    {
                        "autoPlay": true,
                        "loop": true,
                        "volume": 0
                    }
                },

                "camera":
                {
                    "fov":
                    {
                        "default": 180
                    }
                },

                "view":
                {
                    "type": "flat"
                }
            },

            {
                "uid": "scene-3",
                "name": "Martin Karting - Cinema Mode",
                "slug": "martin-karting-cinema",
                "description": "Martin Karting Session Edit",
                "sync":["scene-0", "scene-1", "scene-2"],

                "media":
                {
                    "uid": "media-3",
                    "type": "video",

                    "source":
                    {
                        "format": "equi",
                        "streaming": "dash",
                        "url": "https://cdn.forgejs.org/samples/vrrroom/videos/460045/460045.mpd"
                    },

                    "options":
                    {
                        "autoPlay": true,
                        "loop": true,
                        "volume": 0
                    }
                },

                "camera":
                {
                    "fov":
                    {
                        "default": 180
                    }
                },

                "view":
                {
                    "type": "flat"
                }
            }
        ]
    },

    "plugins":
    {
        "prefix": "../../plugins/",

        "engines":
        [
            {
                "uid": "org.forgejs.previousnextbuttons",
                "url": "PreviousNextButtons/"
            },

            {
                "uid": "org.forgejs.gyroscopebutton",
                "url": "GyroscopeButton/"
            }
        ],

        "instances":
        [
            {
                "uid": "previousnextbuttons",
                "engine": "org.forgejs.previousnextbuttons"
            },

            {
                "uid": "gyrobutton",
                "engine": "org.forgejs.gyroscopebutton",

                "options":
                {
                    "bottom": 10,
                    "left": 10
                }
            }
        ]
    }
}